{
	"name": "@mphennum/utils",
	"version": "0.0.1",
	"description": "javascript utilities for strings, numbers, dates, etc",
	"main": "dist/index.js",
	"scripts": {
		"test": "npm run test:lint && npm run test:unit",
		"test:lint": "eslint src",
		"test:unit": "mocha --require esm --require @babel/register",
		"build": "npm run test && rm -rf dist/* && rollup --config",
		"build:watch": "rollup --config --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mphennum/utils.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"javascript",
		"utility",
		"string",
		"number",
		"date",
		"random"
	],
	"author": "mphennum",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/mphennum/utils/issues"
	},
	"homepage": "https://github.com/mphennum/utils#readme",
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/preset-env": "^7.11.5",
		"@babel/register": "^7.11.5",
		"@rollup/plugin-babel": "^5.2.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"babel-eslint": "^10.1.0",
		"chai": "^4.2.0",
		"eslint": "^7.8.1",
		"esm": "^3.2.25",
		"mocha": "^8.1.3",
		"np": "^6.5.0",
		"rollup": "^2.26.10",
		"rollup-plugin-terser": "^7.0.2"
	}
}
